<%
	String title,result;
	request.setCharacterEncoding("utf-8");
	String age =request.getParameter("age");
	int age1 = Integer.parseInt(age);
	
	if(age1 >= 20){
		title="성인";
		result="가능";
	}else{
		title="미성년자";
		result="불가능";
	}
		response.sendRedirect("responseAge_07.jsp?age="+age+"&title="+title+"&result="+result);
	

%>

